class Main {
    function void main() {
        // Draw ship
        var int xMax, yMax, xHalf, yHalf;
        var Player player; 
        let xMax = 511;
        let yMax = 255;
        let xHalf = xMax / 2;
        let player = Player.new(xHalf - 13, yMax - 16);
        do player.draw();
        do player.dispose();
        return;
    }
}
